 <!-- Main right-->

		<div id="main">
			<div class="container">
				<div class="row">

					<!-- Content -->
					<div id="content" class="8u skel-cell-important">
						<section>
							<header>
								<h2>{{ listDay.titleH2}}</h2>
								
							</header>

                                                          {{ listDay.content|raw}}
						</section>
					</div>

					<!-- Sidebar -->
					<div id="sidebar" class="4u">
						<section>
							<header>
								<h2>Exercices</h2>
							</header>
							<div class="row">
								<section class="6u">
									<ul class="default">
										{% if listDay.images is not null %}
                                                                                {#  <img src="{{ asset(listDay.images.webPath) }}" alt="{{ listDay.images.alt }}">#}
                                                                                {% for image in listDay.images%}
                                                                                    <li>  

                                                                                    <img src="{{asset(image.webPath)}}" alt="{{ image.alt }}" />

                                                                                    </li>


                                                                                {% endfor %}
                                                                             {% endif %}
									</ul>
								</section>

							</div>
						</section>
					</div>
					
				</div>
			</div>
		</div>
	<!-- /Main -->

        
           