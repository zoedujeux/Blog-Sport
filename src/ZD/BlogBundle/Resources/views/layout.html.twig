{% extends "::layout.html.twig" %}


{% block h1 %}{% endblock %}



{% block script %}
     
        $(document).scroll(function(){
            if ($('#nav-wrapper').offset().top > $('#main').offset().top - $('#nav-wrapper').height())
            {
                $('#nav-wrapper').addClass('grey')
            }
            else
            {
                $('#nav-wrapper').removeClass('grey')
            }
            
        });
{% endblock %}